CREATE OR REPLACE FUNCTION ZOESECURITY.ZOEFUN_AUD_UPD_AUD_DDL_RECORD
(iv_audit_log_id in VARCHAR2,in_rerurncode NUMBER)
RETURN INTEGER
AS
  PRAGMA AUTONOMOUS_TRANSACTION;
BEGIN

  UPDATE ZOESECURITY.AUD_DDL_RECORD SET RETURNCODE = in_rerurncode WHERE LOG_ID=iv_audit_log_id;
  COMMIT;
  RETURN 0;
  EXCEPTION
	WHEN OTHERS THEN
    ROLLBACK;
    DBMS_OUTPUT.PUT_LINE('ZOESECURITY.ZOEFUN_AUD_UPD_AUD_DDL_RECORD:'||SQLERRM);
		RETURN -1;

END ZOEFUN_AUD_UPD_AUD_DDL_RECORD;