--添加归档数据库大表抽取进程
add extract exarch1, tranlog, begin now, threads 2
add exttrail ./dirdat/archivetable/ef, extract exarch1

--添加归档数据库除大表外的所有表抽取进程
add extract exarch2, tranlog, begin now, threads 2
add exttrail ./dirdat/archivetable/eg, extract exarch2

--添加归档数据库大表PUMP进程
add extract eparch11, exttrailsource ./dirdat/ef
add rmttrail ./dirdat/archivetable/pf, megabytes 2000, extract eparch11

--添加归档数据库除大表外的所有表PUMP进程
add extract eparch2, exttrailsource ./dirdat/eg
add rmttrail ./dirdat/archivetable/pg, megabytes 2000 ,extract eparch2


--添加归档数据库大表复制进程
add replicat rparch1, exttrail ./dirdat/archivetable/pf, checkpointtable ggsadmin.chkpoint

--添加归档数据库除大表外的所有表复制进程


